<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sukhajata.everydayenglish" version="1.0.101" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Everyday English</name>
    <description>
        An app for learning English.
    </description>
    <author email="timcranfield31@gmail.com" href="http://sukhajata.com">
        Tim Cranfield
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access launch-external="yes" origin="mailto:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
